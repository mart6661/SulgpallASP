@model IEnumerable<Domain.Calender>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Competition.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Match.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Competition.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Match.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CalenderId }) |
